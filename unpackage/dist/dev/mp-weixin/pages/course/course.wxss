page {
  background: #EFEFF4;
  width: 100%;
}
.attention {
  position: relative;
}
.jians {
  position: absolute;
  top: 6rpx;
  right: 6rpx;
  color: red;
}
.coures {
  padding-top: 40rpx;
  position: relative;
}
._textarea {
  width: 700rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 32rpx;
}
.page-section {
  padding-left: 25rpx;
  background: white;
}
.page-section-title {
  border-bottom: 1rpx solid #E5E5E5;
  line-height: 80rpx;
  font-size: 33rpx;
  color: #333333;
}
.textare {
  height: 122rpx;
}
.nums {
  padding-bottom: 16rpx;
  color: #A1A1A1;
  font-size: 28.000000000000004rpx;
  padding-right: 30rpx;
  border-bottom: 1rpx solid #E5E5E5;
  overflow: hidden;
}
.num,
._button {
  float: right;
}
.textarea-wrp {
  padding: 25rpx 0;
}
.picture {
  margin: 18rpx 0;
  overflow: hidden;
}
.uploader-img {
  display: block;
  width: 120rpx;
  height: 120rpx;
}
.uni-uploader {
  float: left;
  position: relative;
  margin-right: 18rpx;
  margin-bottom: 18rpx;
  width: 120rpx;
  height: 130rpx;
  border: 1rpx dashed #D9D9D9;
}
.uploader-input {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  display: -webkit-flex;
}
.uploader-input image {
  width: 80rpx;
  height: 80rpx;
}
.button {
  font-size: 28.000000000000004rpx;
  position: absolute;
  right: 30rpx;
  overflow: hidden;
}
.mini-btn {
  background: #6492FF;
  color: white;
}
.button-hover ._button {
  background: #007AFF;
}